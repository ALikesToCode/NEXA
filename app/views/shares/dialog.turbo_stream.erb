<%= turbo_stream.dialog do
  render(Primer::Alpha::Dialog.new(title: t(:label_share_project_list),
                                   id: 'share_list',
                                   data: { 'keep-open-on-submit': true },
                                   size: :xlarge)) do |d|
    d.with_header(variant: :large)
    d.with_body do
      render(Shares::ModalBodyComponent.new(strategy: @sharing_strategy, shares: @shares, errors: @errors))
    end
  end
end
%>
